<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InterfacePi.Interface.Setting.SettingController">
   <children>
      <Pane fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="1024.0" style="-fx-background-color: black;">
         <children>
            <VBox alignment="CENTER" prefHeight="600.0" prefWidth="1024.0" spacing="40.0">
               <children>
                  <Button id="dark-blue" fx:id="infoRaps" mnemonicParsing="false" prefHeight="100.0" prefWidth="600.0" stylesheets="@../button.css" text="Info et Configuration Raspberry" textAlignment="CENTER">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Button>
                  <Button id="dark-blue" fx:id="gestionModule" mnemonicParsing="false" prefHeight="100.0" prefWidth="600.0" stylesheets="@../button.css" text="Gestion Module(s)" textAlignment="CENTER">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Button>
                  <Button id="dark-blue" fx:id="gestionApp" mnemonicParsing="false" prefHeight="100.0" prefWidth="600.0" stylesheets="@../button.css" text="Gestion Application" textAlignment="CENTER">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <Button fx:id="back" layoutX="20.0" layoutY="20.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: null;" />
         </children>
      </Pane>
   </children>
</VBox>
